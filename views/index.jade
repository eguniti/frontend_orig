doctype html 
html(lang='en')
  head
     p(align= 'centre'): img(src="/images/logo.png", width='500', height='500')
     div
     h2(style= 'margin-left:auto;margin-right:auto;') Nagarro's Employee Information
     table(style='border:1 solid black;margin-left:auto;margin-right:auto;width:50%')
      thead
        tr
          th ID
          th Name
          th Age
          th Address
          th Salary
      tbody
        each srec in employeeData
         tr
          td #{srec.id}
          td #{srec.name}
          td #{srec.age}
          td #{srec.address}
          td #{srec.salary} 